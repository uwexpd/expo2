<% if question.attribute_to_update == 'student_no' && !@user_application.person.is_a?(Student) %>
	n/a
<% else %>
	<% if question.attribute_to_update == "majors_list" %>
		<%= eval "@user_application.person.majors_list(true)" rescue "--" %>
	<% else %>
		<%= eval "@user_application.person.#{question.attribute_to_update}" rescue "--" %>
	<% end %>
<% end -%>
