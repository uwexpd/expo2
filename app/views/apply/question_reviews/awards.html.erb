<% 
non_blank_awards = []
@user_application.awards.each do |award|
	unless award.requested_quarter_id.nil?
		non_blank_awards << award
	end
end

 %>

<%= non_blank_awards.collect{ |award| Quarter.find(award.requested_quarter_id).title }.join(", ")%>


