        <label for="q_email" class="label">Email</label>
        <input type="text" name="q[email_contains]" id="q_email" />
      </div>
      <div class="string input optional stringish filter_form_field filter_string">
        <label for="q_email" class="label">Student Number</label>
        <input type="text" name="q[student_no_eq]" id="q_student_no" />
      </div>
      <div class='buttons'>
        <input type="submit" value="Search" name="commit" id="q_submit" />
      </div>   
</form>
